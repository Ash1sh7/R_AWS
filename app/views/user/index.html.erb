<%title ="User Photos List" %>
<style>

    body {
        background-color: white;
        text-align: center;
        color: cornflowerblue;
    }
    tr:nth-child(even) {
        background-color: #f2f2f2;
    }
</style>

<h1> List Of All Users</h1>
<table>
  <th>USER</th>
---------------------------
  <% for user in @user %>
    <tr>
      <td><%= user.id %></td>
      <td><%= link_to user.username, controller: "photo" , id: user.id, action: "index" %></td>
  <% end %>
  </tr>
</table>
